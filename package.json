{
  "name": "classic-asp",
  "displayName": "Classic ASP",
  "description": "Classic ASP extension is a streamlined VSCode extension specifically designed for Classic ASP applications. It enables developers to quickly locate and jump to function definitions within VBScript in HTML and ASP files, and adds syntax highlighting for VBScript. This integration improves code navigation and reduces the time spent searching through codebases. Ideal for Classic ASP developers, this tool offers a practical solution to enhance productivity.",
  "publisher": "Payaam Emami",
  "version": "0.0.1",
  "main": "./src/extension.js",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "html",
        "aliases": ["HTML"],
        "extensions": [".html"]
      },
      {
        "id": "asp",
        "aliases": ["ASP"],
        "extensions": [".asp"]
      },
      {
        "id": "plaintext",
        "aliases": ["Plain Text"],
        "extensions": [".txt", ".vbs"]
      },
      {
        "id": "vbs",
        "aliases": ["VBScript", "vbs"],
        "extensions": [".vbs"],
        "configuration": "./syntaxes/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "vbs",
        "scopeName": "source.vbs",
        "path": "./syntaxes/vbs.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:asp",
    "onLanguage:plaintext",
    "onLanguage:vbs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PayaamEmami/classic-asp-vscode-extension.git"
  }
}
